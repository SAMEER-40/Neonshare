rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      // Allow read, create, update for everyone to support the current
      // password-based auth implementation and easy testing.
      // WARNING: This is not secure for production with sensitive data.
      allow read, create, update: if true;
      allow delete: if false;
    }
  }
}